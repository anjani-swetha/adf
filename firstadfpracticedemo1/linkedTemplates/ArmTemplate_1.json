							},
							"name": "sink"
						}
					],
					"transformations": [
						{
							"name": "unionByPosition"
						}
					],
					"scriptLines": [
						"source(output(",
						"          Name as string,",
						"          Salary as string,",
						"          DeptNo as string",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     ignoreNoFilesFound: false) ~> Emp",
						"source(output(",
						"          DeptNo as string,",
						"          Dept_Name as string",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     ignoreNoFilesFound: false) ~> Dept",
						"Emp, Dept union(byName: false)~> unionByPosition",
						"unionByPosition sink(allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     input(",
						"          DeptNo as string,",
						"          NoOfPeople as string,",
						"          Dept_Name as string",
						"     ),",
						"     partitionFileNames:['Union_By_Position.csv'],",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true,",
						"     partitionBy('hash', 1)) ~> sink"
					]
				}
			},
			"dependsOn": []
		},
